{% extends "::base.html.twig" %}

{% block title %}ZacjaBundle:User:showUserConquers{% endblock %}

{% block body %}
	{{ render(controller('ZacjaBundle:Common:header')) }}

	<div id="last_conquers_list">
		<ul>
			{% for conquer in conquers %}
				<li>
					<p>
						{{ conquer.score }} points  by
						<a href="/user/{{ conquer.user.username }}">{{ conquer.user.username }}</a>
					</p>
				</li>
			{% endfor %}
		</ul>
	</div>

{% endblock %}
